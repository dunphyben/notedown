template(name='powerNote')
  .note.power-note(class='#{type}')
    ul.table
      li.cell.token-cell
        p= token
      li.cell.note-cell
        .text.markdown!= noteToHTML
        textarea.text-edit.hidden(rows='1')= note
      li.cell.date-cell: p.date= formatDate
      li.cell.button-cell: button.archive(href='#'): i.fa.fa-archive
