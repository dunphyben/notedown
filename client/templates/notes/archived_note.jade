template(name='archivedNote')
  .note.archived
    ul.table
      li.cell.note-cell
        .text.markdown!= noteToHTML
      li.cell.date-cell: p.date= formatDate
      li.cell.button-cell: button.restore(href='#'): i.fa.fa-level-up
      li.cell.button-cell: button.delete(href='#'): i.fa.fa-times
