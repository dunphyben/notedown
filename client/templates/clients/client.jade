template(name='client')
  .client
    ul.table
      li.cell.token-cell: p: i.fa.fa-user
      li.cell.text-cell
        p.text= name
        textarea.text-edit.hidden(rows='1')= name
      li.cell.date-cell: p.date= formatDate
      li.cell.button-cell: button.view(href='#'): i.fa.fa-eye
      li.cell.button-cell: button.archive(href='#'): i.fa.fa-archive
